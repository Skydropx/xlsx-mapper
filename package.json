{
  "name": "xlsx-mapper",
  "version": "0.2.3",
  "description": "Read XLSX to map columns",
  "main": "dist/xlsx-mapper.js",
  "standard": {
    "env": [ "mocha" ]
  },
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --compilers js:babel-core/register --colors ./test/ --name=*.spec.js --recursive",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.22.0",
    "chai": "3.4.1",
    "eslint": "1.7.2",
    "eslint-loader": "1.6.1",
    "mocha": "2.3.4",
    "sinon": "^2.1.0",
    "standard": "^9.0.2",
    "standard-loader": "^6.0.1",
    "webpack": "2.2.1",
    "yargs": "6.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Skydropx/xlsx-mapper.git"
  },
  "keywords": [
    "XLSXMapper"
  ],
  "author": "Krasimir Tsonev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Skydropx/xlsx-mapper/issues"
  },
  "homepage": "https://github.com/Skydropx/xlsx-mapper",
  "dependencies": {
    "xlsx": "^0.8.5",
    "script-loader": "^0.7.0"
  }
}
